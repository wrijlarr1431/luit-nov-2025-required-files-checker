{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "logs:DescribeLogStreams"
            ],
            "Resource": [
                "arn:aws:logs:*:*:log-group:/github-actions/required-files-checker/beta:*",
                "arn:aws:logs:*:*:log-group:/github-actions/required-files-checker/prod:*"
            ]
        }
    ]
}